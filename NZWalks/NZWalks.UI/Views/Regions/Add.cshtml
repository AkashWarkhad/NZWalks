@model NZWalks.UI.Models.AddRegionsViewModel
@{
}

<h1 class="mt-3">Add Region</h1>
<form method="post">
    <div class="mt-3">
        <lable class="form-label">Code</lable>
        <input type="text" class="form-control" asp-for="Code" />
    </div>
    <div class="mt-3">
        <lable class="form-label">Name</lable>
        <input type="text" class="form-control" asp-for="Name" />
    </div>
    <div class="mt-3">
        <lable class="form-label">Image Url</lable>
        <input type="text" class="form-control" asp-for="RegionImageUrl" />
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>